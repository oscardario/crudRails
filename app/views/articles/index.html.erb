<h1 class="text-center">Estoy en el Index</h1>
<% @articles.each do |article| %>
<h2>Escrito por: <%= article.user.email %></h2>
<h4><%= link_to article.title, article %></h4>
  <div>
    <%= article.body %> - <%= link_to 'Eliminar', article, method: :delete %>
    <%= link_to 'Editar', edit_article_path(article) %>
  </div>
<%end%>
